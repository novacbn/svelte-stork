{
    "name": "@novacbn/svelte-stork",
    "description": "Minimalist Svelte wrapper Stores around Stork search, also provides customizable Components.",
    "version": "0.0.5",
    "author": "novacbn",
    "type": "module",
    "main": "package/index.js",
    "typings": "package/types/index.d.ts",
    "exports": {
        ".": "./package/index.js",
        "./components": "./components.js"
    },
    "keywords": [
        "search",
        "stork",
        "svelte",
        "svelte-components",
        "svelte-stores"
    ],
    "files": [
        "package/components",
        "package/stores",
        "package/stork",
        "package/util",
        "package/types",
        "package/index.js",
        "package.json",
        "index.d.ts",
        "components.js",
        "components.d.ts"
    ],
    "scripts": {
        "build": "npm run test && npm run build:lib",
        "build:lib": "svelte-kit package",
        "build:typedoc": "typedoc --out ./package/docs/api --exclude '**/index.ts' --stripInternal --target es6 --theme minimal --mode modules src",
        "test": "npm run test:types && npm run test:svelte",
        "test:svelte": "svelte-check --tsconfig ./tsconfig.json",
        "test:types": "tsc --noEmit"
    },
    "dependencies": {
        "svelte": "^3.38.3"
    },
    "devDependencies": {
        "@sveltejs/kit": "^1.0.0-next.142",
        "@tsconfig/svelte": "^2.0.1",
        "prettier": "^2.3.2",
        "svelte-check": "^2.2.4",
        "svelte-preprocess": "^4.7.4",
        "svelte2tsx": "^0.4.5",
        "typescript": "^4.3.5"
    }
}
